# Railway will automatically set these
NODE_ENV=production
PORT=3000

# Optional: CoinGecko API key for higher rate limits
# COINGECKO_API_KEY=your_api_key_here

# FRED (Federal Reserve Economic Data) API Key
# Get your free API key at: https://fred.stlouisfed.org/docs/api/api_key.html
# This is used to fetch real-time US Treasury rates for risk-free rate calculations
FRED_API_KEY=your_fred_api_key_here

# Treasury API Configuration
# Enable/disable treasury rate fetching (set to false to use fallback rates)
TREASURY_API_ENABLED=true

# Treasury rate cache TTL in milliseconds (24 hours = 86400000)
TREASURY_CACHE_TTL=86400000

# Optional: Custom fallback rates (as percentages, e.g., 4.25 for 4.25%)
# These are used when FRED API is unavailable
# FALLBACK_3M_RATE=4.20
# FALLBACK_6M_RATE=4.23
# FALLBACK_1Y_RATE=4.25
# FALLBACK_2Y_RATE=4.30